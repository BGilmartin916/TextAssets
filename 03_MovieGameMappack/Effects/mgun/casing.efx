//Bullet Shell Casing, falling from the approximate realistic place
//Also bounces on architecture.

Emitter
{
	name				shell

	flags				useModel usePhysics 

	life				1000000

	cullrange			2000

	bounce				0.4 0.2

	angleDelta			0 180 360 180 360 180

	velocity			120 -10 0 100 10 0

	gravity				-300 -200

	origin				-30 0 0 0 0 0

	models
	[
		models/ammo/shell.md3
	]
}