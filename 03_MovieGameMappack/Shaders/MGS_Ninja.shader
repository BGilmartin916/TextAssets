textures/mgs_ninja/Basic_Dark
{ 
	qer_editorimage textures/mgs_ninja/Basic_Dark
      q3map_nonplanar
 	
 	{
 		map $lightmap
 		rgbGen identity
 	}
 	{
 		map textures/mgs_ninja/Basic_Dark
 		blendFunc filter
 	}
    {
        map textures/mgs_ninja/Basic_Dark
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/mgs_ninja/p_Env
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen const 0.25
        tcGen environment
    }
}

