textures/mgs_ninja/piss
{
	qer_editorimage	textures/mgs_ninja/piss
	polygonOffset
	q3map_nolightmap
    {
        map textures/mgs_ninja/piss
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
    }
}

textures/mgs_ninja/ceiling
{
    {
        map $lightmap
    }
    {
        map textures/mgs_ninja/ceiling
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/mgs_ninja/ceiling_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

