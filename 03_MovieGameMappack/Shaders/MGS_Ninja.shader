textures/mgs_ninja/carpet
{
	qer_editorImage textures/mgs_ninja/carpet
	q3map_material	Carpet
	q3map_nonPlanar
	q3map_shadeAngle 120
	{
		map textures/mgs_ninja/carpet
		rgbGen identity
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
