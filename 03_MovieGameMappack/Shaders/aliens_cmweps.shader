models/weapons2/aliens_prifle/prifle
{
	q3map_nolightmap
    {
        map models/weapons2/aliens_prifle/prifle
        rgbGen lightingDiffuse
    }
    {
        map models/weapons2/aliens_prifle/prifle_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

