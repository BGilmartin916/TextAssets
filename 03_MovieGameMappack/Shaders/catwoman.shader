models/players/catwoman/legs
{
	{
		map models/players/catwoman/legs.tga
		rgbGen lightingDiffuse
	}
	{
    
        map gfx/effects/chr_white_add_mild.tga
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        tcGen environment
    	}

  	{
