models/players/colonial_marine/torso_default
{
    {
        map models/players/colonial_marine/torso_default
        rgbGen lightingDiffuse
    }
    {
        map models/players/colonial_marine/torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

