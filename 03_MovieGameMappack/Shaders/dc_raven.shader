models/players/dc_raven/offeecape
{
	q3map_nolightmap
	cull	disable
    {
        map models/players/dc_raven/offeecape
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
}

models/players/dc_raven/offeebody
{
    {
        map models/players/dc_raven/offeebody
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/dc_raven/offeebody_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

