models/players/elven_elite/knight_arms
{
        deformvertexes bulge 0 -0.4 0
    {
        map models/players/elven_elite/knight_arms
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/elven_elite/knight_arms
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

