models\players\harleyquinn\body
{
	cull	twosided
    {
        map models\players\harleyquinn\body
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models\players\harleyquinn\body_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

