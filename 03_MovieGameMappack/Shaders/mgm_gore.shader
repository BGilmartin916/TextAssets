gfx/gore/bullethole_lightblue
{
	qer_editorimage	gfx/gore/bullethole_lightblue
	polygonOffset
    {
        clampmap gfx/gore/bullethole_lightblue
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        alphaGen const 0.7
        rgbGen lightingDiffuse
    }
}

