// Shader for mitsurugi 


models/players/mitsurugi/hakama
{
	cull	disable
    {
        map models/players/mitsurugi/hakama
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
}

models/players/mitsurugi/hakama_blue
{
	cull	disable
    {
        map models/players/mitsurugi/hakama_blue
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
}

models/players/mitsurugi/trainer_legs
{
	q3map_nolightmap
	q3map_onlyvertexlighting
    {
        map models/players/mitsurugi/trainer_legs
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/mitsurugi/trainer_legs_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
     
}

