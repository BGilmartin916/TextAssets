models/players/orc_warchief/armor
{
	cull	disable
    {
        map models/players/orc_warchief/armor
	blendFunc GL_ONE GL_ZERO
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/orc_warchief/armor
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

