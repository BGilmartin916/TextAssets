textures/plasma_ultimate/manhole
{
	qer_editorimage textures/plasma_ultimate/manhole
	qer_trans 1.0
	surfaceparm	nonopaque
	surfaceparm	trans
	polygonOffset
	{
		map textures/plasma_ultimate/manhole
		alphaFunc GE128
		depthWrite
		rgbGen vertex
	}
	{ 
		map $lightmap
		rgbGen identity 
		blendFunc filter 
		depthFunc equal 
	}
}

