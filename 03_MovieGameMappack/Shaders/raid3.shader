textures/raid3_sky/raid3_night_cl
{
	qer_editorimage	textures/raid3_sky/raid3_up.jpg
	surfaceparm	sky
        surfaceparm	noimpact
        surfaceparm	nomarks
	q3map_nolightmap
	q3map_sun 0.846160 0.983126 1.000000 150 90 90
	skyParms	textures/raid3_sky/raid3 512 -
  {
        map textures/raid3_sky/cloudlayer_raid3_night
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
	  tcMod scroll 0 0.0225
        tcMod scale 4 4
    }
}

