textures/raid3_water/water_rough
{
	qer_editorimage	textures/raid3_water/water1
	surfaceparm	nomarks
	surfaceparm	nonsolid
	surfaceparm	nonopaque
	surfaceparm	water
	surfaceparm	trans
	q3map_material	Water
	q3map_onlyvertexlighting
	deformvertexes	wave	1000 sin 1 9 2 0.6
    {
        map $lightmap
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/raid3_water/water1
        blendFunc GL_DST_COLOR GL_ZERO
        tcMod turb 0.01 0.03 5 0.1
        tcMod stretch noise 1 0.01 1 1
    }
    {
        map textures/raid3_water/water1
        blendFunc GL_ONE GL_ONE
        tcMod turb 0.02 -0.06 0 0.4
        tcMod stretch noise 4 1 0 0.1
    }
}

textures/raid3_base/chainlinkfence
{
	surfaceparm	nonsolid
	surfaceparm	nonopaque
	surfaceparm	playerclip
	surfaceparm	monsterclip
	q3map_alphashadow
	cull	disable
    {
        map textures/raid3_base/chainlinkfence
        alphaFunc GE128
        blendFunc GL_SRC_ALPHA GL_ZERO
        depthWrite
    }
    {
        map $lightmap
        blendFunc GL_ONE GL_ZERO
        depthFunc equal
    }
    {
        map textures/raid3_base/chainlinkfence
        blendFunc GL_DST_COLOR GL_ZERO
        depthFunc equal
    }
}

