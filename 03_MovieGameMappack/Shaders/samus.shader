models/players/samus/default_arms
{
	{
		map models/players/samus/default_arms.tga
		rgbGen lightingDiffuse
	}
	{
		map models/players/samus/default_reflect.tga
		blendfunc add
		rgbGen lightingDiffuse
		tcGen environment 
	}
	{
		map models/players/samus/default_arms.tga
		blendfunc blend
		rgbGen lightingDiffuse
	}
	{
		map models/players/samus/default_arms_glow.tga
		blendfunc add
		glow
		rgbGen identityLighting
	}
}

