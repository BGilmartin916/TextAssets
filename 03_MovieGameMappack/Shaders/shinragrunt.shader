models/players/ShinRaGrunt/torso
{
	cull	twosided
    {
        map models/players/ShinRaGrunt/torso
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/ShinRaGrunt/torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

