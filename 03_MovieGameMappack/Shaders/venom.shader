models/players/venom/torso_arms_default
{
    {
        map models/players/venom/torso_arms_default
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map gfx/effects/white
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        tcGen environment
    }
    {
        map models/players/venom/torso_arms_default
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
}

