models/players/Stormie_TFU/body_clean
{
	cull	twosided
	q3map_nolightmap
	{
		map models/players/Stormie_TFU/body_clean
		blendFunc GL_ONE GL_ZERO
		rgbGen lightingDiffuse
	}

	{
		map models/players/Stormie_TFU/body_clean_spec
		blendFunc GL_SRC_ALPHA GL_ONE
