
models/players/CadBane/upper
	{
	{
		map models/players/CadBane/upper
		rgbGen lightingDiffuse
	}
	{
		map models/players/CadBane/upper_e
       		blendFunc GL_ONE  GL_ONE
      		glow
	}
 	{
       		 map models/players/CadBane/upper_spec
       		blendFunc GL_SRC_ALPHA GL_ONE
      		detail
       		alphaGen lightingSpecular
	}
	}

