models/players/Chirrut/torso
{
	q3map_nolightmap
	cull twosided
	{
		map models/players/Chirrut/torso
		rgbGen lightingDiffuse
	}
	{
		map models/players/Chirrut/torso_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}

models/players/Chirrut/hip_parts
{
	q3map_nolightmap
	cull twosided
	{
		map models/players/Chirrut/hip_parts
		rgbGen lightingDiffuse
	}
	{
		map models/players/Chirrut/hip_parts_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
}