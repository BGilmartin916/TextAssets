models/players/dt_imp_comm/armor
{
    {		
        map models/players/dt_imp_comm/armor_A
        normalMap models/players/dt_imp_comm/armor_N
	rmoMap models/players/dt_imp_comm/armor_RMO
	rgbGen lightingDiffuse
    }    
    {
        map models/players/dt_imp_comm/armor_e
        blendFunc GL_ONE GL_ONE
        glow
        detail
        rgbGen identity
    }
}

