
// Heavy Mandos

models/players/mandalorian_heavy/tyton_armor
{
	q3map_nolightmap
	cull	disable
    {
        map models/players/mandalorian_heavy/tyton_armor
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/mandalorian_heavy/tyton_armor-spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

