models/weapons2/mando_rifle/rifle_diff
{
    {
        map models/weapons2/mando_rifle/rifle_diff
        alphaFunc GE128
        rgbGen lightingDiffuse
    }
    {
        map models/weapons2/mando_rifle/rifle_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
        detail
    }

}

