models/map_objects/mb2_tutorial/MB2Logo
{
	{
		map $lightmap
	}
	{
		map models/map_objects/mb2_tutorial/MB2Logo_A
		normalMap models/map_objects/mb2_tutorial/MB2Logo_N
		rmoMap models/map_objects/mb2_tutorial/MB2Logo_RMO
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

models/map_objects/mb2_tutorial/Alphabet
{
	{
		map $lightmap
	}
	{
		map models/map_objects/mb2_tutorial/Alphabet_A
		rmoMap models/map_objects/mb2_tutorial/Alphabet_RMO
		blendFunc GL_DST_COLOR GL_ZERO
	}
}