models/players/pazvizsla/vizla_armor
{
	q3map_nolightmap
	cull	disable
    {
        map models/players/pazvizsla/vizla_armor
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/pazvizsla/vizla_armor-spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

