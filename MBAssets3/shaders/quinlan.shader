models/players/quinlan/head_hair
{	
	{
		map models/players/quinlan/head_hair
		rgbGen lightingDiffuse
	}
	{
		map models/players/quinlan/head_hair_specular
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen lightingSpecular
	detail
	}
}

