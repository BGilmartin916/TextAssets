models/players/reborn_concept/head_face
{
    {
        map models/players/reborn_concept/head_face
        rgbGen lightingDiffuse
    }
    {
        map models/players/reborn_concept/head_face_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/players/reborn_concept/head_face_glow
        blendFunc GL_ONE GL_ONE
    }
}

