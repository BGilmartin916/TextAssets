models/players/scouttrooper/helmet
{
    {
        map models/players/scouttrooper/helmet
        rgbGen lightingDiffuse
    }
    {
        map models/players/scouttrooper/helmet_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

