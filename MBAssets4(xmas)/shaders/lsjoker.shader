models/players/lsjoker/coat_blue
{
	cull	twosided
    {
        map models/players/lsjoker/coat_blue
        blendFunc GL_DST_ALPHA GL_ZERO
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/lsjoker/coat_blue
        blendFunc GL_SRC_ALPHA GL_SRC_COLOR
        detail
        alphaGen lightingSpecular
    }
}

