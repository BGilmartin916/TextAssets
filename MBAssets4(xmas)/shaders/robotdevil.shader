models/players/robotdevil/teeth
{
	cull	twosided
    {
        map models/players/robotdevil/teeth
	blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
	rgbGen lightingDiffuse
    }
}

models/players/robotdevil/arms
{
	q3map_nolightmap
	q3map_onlyvertexlighting
    {
        map models/players/robotdevil/arms
        rgbGen lightingDiffuse
    }
    {
        map models/players/robotdevil/arms_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

