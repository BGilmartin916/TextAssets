models/players/bxdroid/torsozor_jay
{
    {
        map models/players/bxdroid/torsozor_jay
	blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/droideka/metal_env
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/droideka/metal_env2
        blendFunc GL_ONE GL_ONE
        tcGen environment
    }
    {
        map models/players/bxdroid/torsozor_spec_jay
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
    {	
        map models/players/bxdroid/torsozor_glow
        blendFunc GL_ONE GL_ONE
        rgbGen wave square 0 1 0 2
    }
}

