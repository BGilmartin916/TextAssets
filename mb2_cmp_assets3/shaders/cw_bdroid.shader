models/players/cw_bdroid/head
{
	q3map_nolightmap
    {
        map models/players/cw_bdroid/head
        rgbGen identity
    }
}

models/players/cw_bdroid/hips
{
	q3map_nolightmap
	cull	disable
    {
        map models/players/cw_bdroid/hips
        rgbGen identity
    }
    {
        map models/players/cw_bdroid/shade
        blendFunc GL_DST_COLOR GL_ZERO
        rgbGen identity
        tcGen environment
    }
}

