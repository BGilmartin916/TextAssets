models/players/cw_sbdroid/torso
{
	q3map_nolightmap
    {
        map models/players/cw_sbdroid/torso
        rgbGen identity
    }
    {
        map models/players/cw_sbdroid/shade
        blendFunc GL_DST_COLOR GL_ZERO
        rgbGen identity
        tcGen environment
    }
}

