textures/flowers/flowergrass
{
	qer_editorimage   gfx/sprites/flowerone
	q3map_material   ShortGrass
	cull   twosided
	{
		map $lightmap
	}
	{
		map textures/newtheed2/flowergrass
		blendFunc GL_DST_COLOR GL_ZERO
	}
	{
		map gfx/sprites/flowerone
			surfaceSprites vertical 32 24 42 500
			ssFademax 4000
			ssFadescale 1
			ssVariance 1 2
			ssWind 0.5
		alphaFunc GE192
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		depthWrite
		rgbGen vertex
	}
}


