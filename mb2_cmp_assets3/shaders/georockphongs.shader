 textures/CMP_arena/sandstone
 {
 	q3map_nonplanar
 	q3map_shadeangle 60
 	qer_editorimage textures/CMP_arena/sandstone
 	{
 		map $lightmap
 		rgbGen identity
 	}
 	{
 		map textures/CMP_arena/sandstone
 		blendFunc filter
 	}
 }

