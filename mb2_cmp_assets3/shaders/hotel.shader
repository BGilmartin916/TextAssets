textures/hotel/blackyellowmarble
{
	qer_editorimage	textures/hotel/blackyellowmarble
	q3map_material	SolidMetal
	{
		map textures/imperial/etest3
		tcGen environment
	}
	{
		map textures/hotel/MARBLE
		blendFunc GL_SRC_ALPHA GL_SRC_ALPHA
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

