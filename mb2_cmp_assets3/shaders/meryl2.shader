models/players/meryl2/hair
{
	cull	disable
    {
        map models/players/meryl2/hair
        alphaFunc GE192
        rgbGen lightingDiffuse
    }
}

models/players/meryl2/eyeshadow
{
	cull	disable
    {
        map models/players/meryl2/head
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
}

models/players/meryl2/eyelashes
{
	cull	disable
    {
        map models/players/meryl2/head
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
}

models/players/meryl2/head
{
	cull	disable
    {
        map models/players/meryl2/head
        rgbGen lightingDiffuse
    }
}