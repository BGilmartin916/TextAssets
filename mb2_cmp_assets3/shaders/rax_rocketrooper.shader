models/players/rax_rockettrooper/rockettrooper_torso
{
	cull	twosided

	{
		map models/players/rax_rockettrooper/rockettrooper_torso
		rgbGen lightingDiffuse
	}
   	{
		map models/players/SBD/chrome4.tga
		blendfunc gl_src_alpha gl_one
		tcGen environment 
	}
	{
        map models/players/rax_rockettrooper/rockettrooper_torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }

}

