models/players/st_swamptrooper/helmet
{
	{
		map models/players/st_swamptrooper/helmet
		rgbGen lightingDiffuse
	}
	{
		map gfx/effects/chr_inv
		blendFunc GL_DST_COLOR GL_SRC_COLOR
		tcGen environment

	}
	{
		map models/players/st_swamptrooper/helmet_spec
		blendFunc GL_SRC_ALPHA GL_ONE
		detail
		alphaGen lightingSpecular
	}
	{
		map models/players/st_swamptrooper/helmet_glow
		blendFunc GL_ONE GL_ONE
		rgbGen wave sin 0.5 0.5 0.5 0.5
		glow
	}
}

