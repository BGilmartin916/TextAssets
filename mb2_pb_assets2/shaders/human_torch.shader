models/players/human_torch/torso2
{
	cull	disable
    {
        map models/players/human_torch/torso2
        alphaFunc GE128
        rgbGen wave sin 1 0.25 0 10
    }
    {
        map models/players/human_torch/flame
        blendFunc GL_ONE GL_ONE
        rgbGen identity
        tcGen environment
        tcMod scroll 4 1
    }
}

