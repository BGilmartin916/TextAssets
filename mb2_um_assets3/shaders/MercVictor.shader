models/players/victor/victor_torso
{
	cull disable
    {
        map models/players/victor/victor_torso
        rgbGen lightingDiffuse
    }
    {
        map models/players/victor/victor_torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

