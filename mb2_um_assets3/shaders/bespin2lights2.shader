textures/bespin/n_win01
{
	qer_editorimage	textures/bespin/n_win01
	q3map_shadeangle	120
	q3map_nonplanar
    {
        map $lightmap
        rgbGen identity
    }
    {
        map textures/bespin/n_win01
        blendFunc GL_DST_COLOR GL_ZERO
    }
    {
        map textures/bespin/n_win01glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

