models/players/darktrooperphs1_prison/eyes
{
	q3map_material	HollowMetal
	q3map_nolightmap
	cull	disable
    {
        map models/players/darktrooperphs1_prison/eyes
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
        alphaGen lightingSpecular
    }
    {
        map models/players/darktrooperphs1_prison/eyesglow
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        rgbGen lightingDiffuse
        alphaGen const 1
    }
}

