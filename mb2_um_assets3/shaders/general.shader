models/players/general/saboteur_head_blue
{
    {
        map models/players/general/saboteur_head_blue
        rgbGen lightingDiffuse
    }
    {
        map models/players/general/chr_inv
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/general/saboteur_head_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

