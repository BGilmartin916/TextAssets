models/players/lasertagfemale/face
{
	q3map_nolightmap
    {
        map models/players/lasertagfemale/face
        alphaFunc GE192
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/lasertagfemale/face
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
}


