models/players/legionary/saboteur_torso_blue
{
	q3map_nolightmap
	q3map_onlyvertexlighting
    {
        map models/players/legionary/saboteur_torso_blue
        rgbGen lightingDiffuse
    }
    {
        map models/players/legionary/chr_inv
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/legionary/saboteur_torso_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

