models/players/rebel_marine/comdohelm
{
    {
        map models/players/rebel_marine/comdohelm
        rgbGen lightingDiffuse
    }
    {
        map models/players/rebel_marine/comdohelm-spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/players/rebel_marine/comdohelm_glow
        blendFunc GL_ONE GL_ONE
        glow
    }
}

models/players/rebel_marine/comxparts
{
	cull	twosided
    {
        map models/players/rebel_marine/comxparts
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/rebel_marine/comxparts-spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

