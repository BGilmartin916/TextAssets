models/players/stormtrooper_knight/arms
{
	cull	twosided
    {
        map models/players/stormtrooper_knight/arms
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/stormtrooper_knight/arms
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

